    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user",
                "description": "Returns token JWT for User",
                "operationId": "login",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResource"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnprocessableLogin"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout user",
                "description": "Destroy token of the user",
                "operationId": "logout",
                "responses": {
                    "204": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/auth/refresh": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Refresh user",
                "description": "Refresh token of the user",
                "operationId": "refresh",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/email/verification-notification": {
            "post": {
                "tags": [
                    "Email"
                ],
                "summary": "Send email for confirmation user",
                "description": "Send email for confirmation user",
                "operationId": "verification-notification",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/EmailResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/email/verify-email/{id}/{hash}": {
            "get": {
                "tags": [
                    "Email"
                ],
                "summary": "Confirmation user",
                "description": "Confirmation user",
                "operationId": "verify-email",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "user id",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "hash",
                        "in": "path",
                        "description": "hash",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/EmailResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/password/forgot-password": {
            "post": {
                "tags": [
                    "Password"
                ],
                "summary": "Forgot password",
                "description": "Forgot password",
                "operationId": "forgot-password",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PasswordRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PasswordResource"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnprocessablePassword"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/password/reset-password": {
            "post": {
                "tags": [
                    "Password"
                ],
                "summary": "Forgot password",
                "description": "Reset password",
                "operationId": "reset-password",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PasswordResetRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PasswordResource"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Content",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UnprocessablePassword"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Error": {
                "title": "Error Resource",
                "description": "Error resource",
                "properties": {
                    "error": {
                        "title": "error",
                        "description": "Error",
                        "type": "string"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "ErrorResource"
                }
            },
            "User": {
                "title": "Users",
                "description": "User model",
                "properties": {
                    "id": {
                        "title": "ID",
                        "description": "ID",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "name": {
                        "title": "Name",
                        "description": "Name of the new user",
                        "type": "string",
                        "example": "Matheus S. Jose"
                    },
                    "email": {
                        "title": "Email",
                        "description": "E-mail of the new user",
                        "type": "string",
                        "example": "This is new email user"
                    },
                    "created_at": {
                        "title": "Created at",
                        "description": "Created at",
                        "type": "string",
                        "format": "datetime",
                        "example": "2020-01-27 17:50:45"
                    },
                    "updated_at": {
                        "title": "Updated at",
                        "description": "Updated at",
                        "type": "string",
                        "format": "datetime",
                        "example": "2020-01-27 17:50:45"
                    },
                    "deleted_at": {
                        "title": "Deleted at",
                        "description": "Deleted at",
                        "type": "string",
                        "format": "datetime",
                        "example": "2020-01-27 17:50:45"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "User"
                }
            },
            "LoginRequest": {
                "title": "Login user request",
                "description": "Login user request body data",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "title": "email",
                        "description": "Email of the user",
                        "type": "string",
                        "example": "matheus@gmail.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "Passowrd of the user",
                        "type": "string",
                        "example": "123456789"
                    }
                },
                "type": "object"
            },
            "PasswordRequest": {
                "title": "Password user request",
                "description": "Password user request body data",
                "required": [
                    "email"
                ],
                "properties": {
                    "email": {
                        "title": "email",
                        "description": "Email of the user",
                        "type": "string",
                        "example": "email@gmail.com.br"
                    }
                },
                "type": "object"
            },
            "PasswordResetRequest": {
                "title": "Password user request",
                "description": "Password user request body data",
                "required": [
                    "email",
                    "password",
                    "password_confirmation",
                    "token"
                ],
                "properties": {
                    "email": {
                        "title": "email",
                        "description": "Email of the user",
                        "type": "string",
                        "example": "matheus@gmail.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "Password of the user",
                        "type": "string",
                        "example": "123456789"
                    },
                    "password_confirmation": {
                        "title": "password_confirmation",
                        "description": "Confirmation password of the user",
                        "type": "string",
                        "example": "123456789"
                    },
                    "token": {
                        "title": "token",
                        "description": "Token for reset password",
                        "type": "string",
                        "example": "441c6dbacfebdc913f5302cbc7212dde99d6aa41c271aafe9260f18256c2edf0"
                    }
                },
                "type": "object"
            },
            "EmailResource": {
                "title": "Email Resource",
                "description": "Email resource",
                "properties": {
                    "message": {
                        "title": "message",
                        "description": "Message",
                        "type": "string"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "EmailResource"
                }
            },
            "LoginResource": {
                "title": "Login Resource",
                "description": "Login resource",
                "properties": {
                    "token": {
                        "title": "Token",
                        "description": "Token wrapper",
                        "type": "string"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "LoginResource"
                }
            },
            "UnprocessableLogin": {
                "title": "Unprocessable login",
                "description": "Unprocessable Content",
                "properties": {
                    "message": {
                        "title": "message",
                        "description": "Error Message",
                        "type": "string"
                    },
                    "errors": {
                        "$ref": "#/components/schemas/UnprocessableLoginErrors"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Unprocessable"
                }
            },
            "UnprocessableLoginErrors": {
                "title": "Unprocessable login errors",
                "description": "Unprocessable Content",
                "properties": {
                    "email": {
                        "title": "email",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "password": {
                        "title": "password",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Unprocessable"
                }
            },
            "PasswordResource": {
                "title": "PasswordResource user",
                "description": "PasswordResource Content",
                "properties": {
                    "status": {
                        "title": "status",
                        "description": "Status Message",
                        "type": "string"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "PasswordResource"
                }
            },
            "UnprocessablePassword": {
                "title": "Unprocessable login",
                "description": "Unprocessable Content",
                "properties": {
                    "message": {
                        "title": "message",
                        "description": "Error Message",
                        "type": "string"
                    },
                    "errors": {
                        "$ref": "#/components/schemas/UnprocessablePasswordErrors"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Unprocessable"
                }
            },
            "UnprocessablePasswordErrors": {
                "title": "Unprocessable login errors",
                "description": "Unprocessable Content",
                "properties": {
                    "email": {
                        "title": "email",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Unprocessable"
                }
            }
        },
        "securitySchemes": {
            "bearer_token": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}